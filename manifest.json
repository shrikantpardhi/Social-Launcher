{
  "manifest_version": 2,

  "name": "Social Shortcut",
  "description": "Shortcut for your social media profiles",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "launch.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}